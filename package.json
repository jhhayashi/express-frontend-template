{
  "name": "express-frontend-template",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "predev": "if [ -f .grunt_pid ]; then kill $(cat .grunt_pid) || true && rm .grunt_pid; fi",
    "dev": "grunt & echo $! > .grunt_pid && npm start"
  },
  "dependencies": {
